{
  "name": "photo_app",
  "version": "1.0.0",
  "description": "An app where you can upload and share photos",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "client-install": "npm install --prefix client",
    "start-dev": "nodemon --exec babel-node server.js",
    "build": "webpack --mode production",
    "serve": "node ./build/server_bundle.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run start-dev\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jgroenke24/photo_app.git"
  },
  "author": "Jorge Groenke",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jgroenke24/photo_app/issues"
  },
  "homepage": "https://github.com/jgroenke24/photo_app#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.13.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-promise-router": "^3.0.3",
    "express-validator": "^5.3.1",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.4.0",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nodemailer": "^5.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.7.1",
    "pug": "^2.0.3",
    "uuid": "^3.3.2"
  }
}
